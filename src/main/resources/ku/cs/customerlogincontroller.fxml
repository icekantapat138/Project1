<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #333333;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.admin.AdminLoginController">
   <children>
      <Label layoutX="313.0" layoutY="46.0" text="Customer Login" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="29.0" />
         </font>
      </Label>
      <HBox layoutX="200.0" layoutY="160.0" prefHeight="43.0" prefWidth="400.0" spacing="20.0">
         <children>
            <Label text="Username  : " textFill="WHITE">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
            </Label>
            <TextField fx:id="usernameTextField" prefHeight="39.0" prefWidth="245.0" promptText="username" />
         </children>
      </HBox>
      <HBox fx:id="passwordField" layoutX="208.0" layoutY="238.0" prefHeight="43.0" prefWidth="384.0" spacing="20.0">
         <children>
            <Label text="Password :" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
            </Label>
            <TextField fx:id="usernameTextField1" prefHeight="39.0" prefWidth="245.0" promptText="password" />
         </children>
      </HBox>
      <Button fx:id="loginBtn" layoutX="271.0" layoutY="379.0" mnemonicParsing="false" text="Login" />
      <Button fx:id="changepasswordBtn" layoutX="417.0" layoutY="379.0" mnemonicParsing="false" text="Change Password" />
      <Button fx:id="backBtn" layoutX="53.0" layoutY="541.0" mnemonicParsing="false" text="Back" />
      <Button fx:id="createaccBtn" layoutX="380.0" layoutY="473.0" mnemonicParsing="false" text="Create Account" />
   </children>
</AnchorPane>
